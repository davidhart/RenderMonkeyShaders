    <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="vs_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 matViewProjection;
float4x4 matView;

float4 lightPos;
float4 eyePos;

struct VS_INPUT 
{
   float4 Position : POSITION0;
   float2 TexCoord : TEXCOORD0;
   
   float3 Normal : NORMAL0;
   float3 Binormal : BINORMAL0;
   float3 Tangent : TANGENT0;
};

struct VS_OUTPUT 
{
   float4 Position : POSITION0;
   float2 TexCoord : TEXCOORD0;
   
   float3 LightDir : TEXCOORD1;
   float3 ViewDir : TEXCOORD2;
   
};

VS_OUTPUT vs_main( VS_INPUT Input )
{
   VS_OUTPUT Output;

   Output.Position = mul ( Input.Position, matViewProjection );
   
   float3 Normal = normalize(Input.Normal);
   float3 Binormal = normalize(Input.Binormal);
   float3 Tangent = normalize(Input.Tangent);
   
   float3x3 view2Tangent = {Tangent, Binormal, Normal};
   
   Output.LightDir = mul(view2Tangent, lightPos.xyz - Input.Position.xyz);
   Output.ViewDir = mul(view2Tangent, eyePos.xyz - Input.Position.xyz);   
   Output.TexCoord = Input.TexCoord;
   
   return( Output );
}



]]></RmShaderCode>
