   float2 TexCoord : TEXCOORD0;
   float4 Color : COLOR0;
   
};

VS_OUTPUT vs_main( VS_INPUT Input )
{
   VS_OUTPUT Output;

   Output.Position = mul ( Input.Position, matViewProjection );
   
   float3 lightDir = normalize ( lightPos.xyz - Input.Position.xyz );
   float3 N = normalize ( Input.Normal );
   
   float3 rLightDir = reflect(-lightDir, N);
   float3 viewDir = normalize ( eyePos.xyz - Input.Position.xyz );
   
   float NDotL = max ( dot( lightDir, N ), 0 );
   float NDotV = max ( dot( rLightDir, viewDir ), 0 );
   
   float3 col = Kd * diffuseLight * NDotL;
   col += Ks * specularLight * pow(NDotV, specularPower);
   col += Ka * ambientLight;
   
   Output.TexCoord = Input.TexCoord;
   Output.Color = float4(col, 1.0f);
   
   return( Output );
}



