    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_3_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float4 Ka;
float4 Ks;
float4 Kd;

float4 ambientLight;
float4 diffuseLight;
float4 specularLight;
float specularPower;
float stepSize;
float heightScale;
float paraScale;

sampler2D Texture0;
sampler2D Texture1;

struct PS_INPUT
{
   float2 TexCoord : TEXCOORD0;
 
   float3 LightDir : TEXCOORD1;
   float3 ViewDir : TEXCOORD2;
};

float4 ps_main(PS_INPUT input) : COLOR0
{  
   float3 lightDir = normalize(input.LightDir);
   
   float height = tex2D(Texture1, input.TexCoord).r;
   float3 viewDir = normalize(input.ViewDir);
   
   //float3 N = tex2D(Texture1, input.TexCoord).rgb * 2.0f - 1.0f;
   float2 tc = input.TexCoord + viewDir.xy * height * heightScale * paraScale;
   height = tex2D(Texture1, tc).r;
   float heightX = tex2D(Texture1, tc + float2(stepSize, 0)).r;
   float heightY = tex2D(Texture1, tc + float2(0, stepSize)).r;
   float dFx = heightX - height;
   float dFy = heightY - height;
   float3 N = normalize( float3(-dFx * heightScale, -dFy * heightScale, 1.0) );
   
   //float3 N = float3(0, 0, 1);
   float3 rLightDir = reflect(-lightDir, N);
   
   float NDotL = max ( dot( lightDir, N ), 0 );
   float NDotV = max ( dot( rLightDir, viewDir ), 0 );

   float3 col = Kd * diffuseLight * NDotL;
   col += Ks * specularLight * pow(NDotV, specularPower);
   col += Ka * ambientLight;


   return( float4( tex2D(Texture0, tc).rgb * col, 1.0f ) );
   
}



]]></RmShaderCode>
